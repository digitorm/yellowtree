{% if customed %}
<img class="lazyloaded lazy2 {{ class }}"
     src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAIAAAABCAQAAABeK7cBAAAAC0lEQVR42mNkAAIAAAoAAv/lxKUAAAAASUVORK5CYII=" 
     data-widths="[540, 720, 900, 1080, 1296, 1512, 1728, 1944, 2048]"
     data-aspectratio="{{ image.aspect_ratio }}"
     data-sizes="auto"
     data-parent-fit="cover"
     alt="Product Image"
     data-srcset="{{ my_img_url }}"
     width="{{ image.width }}"
     height="{{ image.height }}"
     >
{% else %}
<img class="lazyloaded lazy2 {{ class }}"
     src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAIAAAABCAQAAABeK7cBAAAAC0lEQVR42mNkAAIAAAoAAv/lxKUAAAAASUVORK5CYII=" 
     data-src="{{ image | img_url: '' }}"
     data-widths="[540, 720, 900, 1080, 1296, 1512, 1728, 1944, 2048]"
     data-aspectratio="{{ image.aspect_ratio }}"
     data-sizes="auto"
     data-parent-fit="cover"
     alt="{{ image.alt | escape }}"
     data-srcset="{{ image | img_url: '400x' }} 360w,
                  {{ image | img_url: '400x' }} 540w,
                  {{ image | img_url: '400x' }} 720w,
                  {{ image | img_url: '400x' }} 900w,
                  {{ image | img_url: '1600x' }} 1600w,
                  {{ image | img_url: '1900x' }} 1900w"
     width="{{ image.width }}"
     height="{{ image.height }}"
     >
{% endif %}